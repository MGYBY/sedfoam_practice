FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      gamma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform  0;// 0.0;

boundaryField
{
    Inlet
    {
        type            groovyBC;
        value           uniform 0;
        valueExpression "pos().z<=(nd+sedH) ? 1.0 : 0.0";
        variables       (
            "nd=13.5322;"
            "nv=0.57609;"
            "sedH=3.0;"
            "cellSizeMod=0.40/8.0/2.0;"
            "sedConc=0.60;"
        );
    }

    Outlet
    {
        type            zeroGradient;
    }

    Back
    {
        type            zeroGradient;
    }

    Symmetry
    {
        type            symmetryPlane;
    }

    Cylinder
    {
        type            zeroGradient;
    }

    Atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    Bottom
    {
        type            zeroGradient;
    }

}


