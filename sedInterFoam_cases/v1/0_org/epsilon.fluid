FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-10; 

boundaryField
{
    Inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    Outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    Back
    {
        type            zeroGradient;
    }

    Bottom
    {
       type            epsilonWallFunction; //zeroGradient;
        value           $internalField;
        blending        exponential;
    }

    Cylinder
    {
        type            epsilonWallFunction; //zeroGradient;
        value           $internalField;
        blending        exponential;

    }

    Atmosphere
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    Symmetry
    {
        type            symmetryPlane;
    }

}


