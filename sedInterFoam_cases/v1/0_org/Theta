FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "50";
    object      Theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-10;//0.; 

boundaryField
{
    Inlet
    {
        type            zeroGradient;
    }

    Outlet
    {
        type            zeroGradient;
    }

    Back
    {
        type            zeroGradient;
    }

    Symmetry
    {
        type            symmetryPlane;
    }

    Cylinder
    {
        type            zeroGradient;
    }

    Atmosphere
    {
        // type            inletOutlet;
        // inletValue      uniform 0;
        // value           uniform 0;
        type            zeroGradient;
    }

    Bottom
    {
        type            zeroGradient;
    }
}


